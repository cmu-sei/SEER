<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
  
    <AssemblyName>Seer</AssemblyName>
    <RootNamespace>Seer</RootNamespace>
    <Version>3.0.0.0</Version>
    <AssemblyVersion>3.0.0.0</AssemblyVersion>
    <FileVersion>3.0.0.0</FileVersion>

    <Authors>RSS Development Team</Authors>
    <Company>Carnegie Mellon University</Company>
    <Product>SEER</Product>
    <Description>System (for) Event Evaluation Research - please email ddupdyke@sei.cmu.edu with bugs/requests/other</Description>
